          <% if current_user != user %>
            <b style='font-size: 22px;'><%= user.name %>
            <%= user.email %></b>
            <%= link_to "Delete", static_pages_destroy_path(user.id),
                          data: { confirm: "Are you sure?" },
                          method: :delete,
                          class: 'btn btn-danger' %>
          <% end %>
        <% end %>
      </div>
    </div>
